{
	"name": "characterwritting",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
  		"dev": "concurrently \"vite\" \"npm run watch:css\"",
		"build": "vite build",
		"preview": "vite preview",
		"build:css": "tailwindcss -i ./src/assets/input.css -o ./src/assets/output.css",
		"watch:css": "tailwindcss -i ./src/assets/input.css -o ./src/assets/output.css --watch"
	},
	"dependencies": {
		"axios": "^1.7.2",
		"pinia": "^2.2.8",
		"vue": "^3.4.29",
		"vue-router": "^4.3.3"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^5.0.5",
		"tailwindcss": "^3.4.4",
		"vite": "^5.3.1"
	}
}
